{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "node",
		"sourceMap": true,
		"jsx": "react",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noEmit": true,
		// "noUnusedLocals": true,
		// "noUnusedParameters": true,
		"noImplicitReturns": true,
		"lib": [
			"WebWorker",
			"ESNext",
		],
		"types": [
			"offscreencanvas",
			"lodash"
		],
		"strict": true
	},
	"include": [
		"./*"
	]
}